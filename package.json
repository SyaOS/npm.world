{
  "name": "npm.world",
  "version": "0.0.0",
  "description": "Yet another npm search engine.",
  "main": "app/index.js",
  "scripts": {
    "start": "node app",
    "test": "standard",
    "dev": "npm start & npm run fe:dev-server",
    "fe:dev-server": "webpack-dev-server --inline --hot --content-base public/",
    "fe:build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SyaOS/npm.world.git"
  },
  "keywords": [
    "npm"
  ],
  "author": "George Chung <gerhut@sya.org.cn>",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/SyaOS/npm.world/issues"
  },
  "homepage": "https://github.com/SyaOS/npm.world#readme",
  "standard": {
    "ignore": [
      "/public/**",
      "/src/main.js"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "bootstrap": "^4.0.0-alpha.2",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.21.0",
    "koa-webpack-dev-middleware": "^1.2.1",
    "koa-webpack-hot-middleware": "^1.0.3",
    "node-sass": "^3.8.0",
    "pug": "^2.0.0-beta3",
    "pug-loader": "^1.0.2",
    "sass-loader": "^3.2.1",
    "standard": "^7.1.2",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.0",
    "vue": "^2.0.0-alpha.2",
    "vue-loader": "^9.0.0",
    "vue-style-loader": "^1.0.0",
    "webpack": "^2.1.0-beta.7",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "koa": "^1.2.0",
    "koa-static": "^2.0.0"
  }
}
